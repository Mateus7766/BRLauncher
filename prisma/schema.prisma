generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "sqlite"
}

model Account {
  id              Int     @id @default(autoincrement())
  access_token    String
  client_token    String
  meta            String
  user_properties String
  uuid            String
  name            String
  selected        Boolean
  type            String
}

model Launcher {
  path     String
  id       Int     @id @default(autoincrement())
  width    Int
  height   Int
  min      Int
  max      Int
  javaPath String?
  elyBy Boolean
}
